# prompt
export PS1="$ "

# some needed var constants
export WINHOME=`cygpath -p -w "${HOME}"` # HOME in non-cygwin style

# machine-depended env vars
export LC_ALL=C
export GIT_EDITOR=OpenInEmacsWaitCygwin.sh
export emacsexe="c:/software/emacs/current/bin/emacs.exe"

# completion
echo "denk an die bash_completion!!!"
# [ -f /usr/local/etc/bash_completion ] && . /usr/local/etc/bash_completion

export PYTHONPATH=`cygpath -p -u "$PYTHONPATH"`

# aliases
alias mklink="cmd.exe /c mklink" 
alias rd="cmd.exe /c rmdir" 
alias k9="kill -9"

. ~/.protected/bashrc_acc
